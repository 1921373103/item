<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lin.item.dao.PhoneCardDao">


    <update id="updateByCampusTelephoneCard">
        update phone_card
        <trim prefix="SET" suffixOverrides=",">
            <if test="card_status != null">card_status = #{cardStatus},</if>
        </trim>
        where campus_telephone_card = #{campusTelephoneCard}
    </update>
</mapper>